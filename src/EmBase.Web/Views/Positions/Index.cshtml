@model List<EmBase.Enities.Positions>
@{
    ViewBag.Title = "Stanowisko";
}

<h2>Stanowisko</h2>

<section id="control">
  <p>
    <a href="/Position/Add" class="btn btn-default">
      Dodaj Stanowisko
      <span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span>
    </a>
  </p>

</section>

<section id="data">
  <table class="table">
    <tr>
      <th>Nazwa Stanowiska</th>
      <th></th>
      <th></th>
    </tr>
    @foreach (var modele in Model)
      {
      <tr>
        <td>
          @modele.Position_name
        </td>

        <td>
          <a href="/Positon/Edit?symbol=@modele.Id_Position" class="btn btn-default">
            Edycja  Stanowisko
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          </a>
        </td>
      </tr>
    }
  </table>
</section>